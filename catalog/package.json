{
  "name": "quilt-navigator",
  "version": "0.0.0",
  "description": "Quilt Navigator web-app",
  "repository": {
    "type": "git",
    "url": "git://github.com/quiltdata/quilt.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8.10.0"
  },
  "author": "Quilt Data, Inc. <contact@quiltdata.io> (https://quiltdata.com)",
  "license": "MIT",
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "npm run analyze:clean",
    "analyze": "node ./internals/scripts/analyze.js",
    "bundlesize": "bundlesize -f './build/*.js'",
    "extract-intl": "node ./internals/scripts/extract-intl.js",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "start": "cross-env NODE_ENV=development node server",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "presetup": "npm i chalk shelljs",
    "setup": "node ./internals/scripts/setup.js",
    "clean": "shjs ./internals/scripts/clean.js",
    "clean:all": "npm run analyze:clean && npm run test:clean && npm run build:clean",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "npm run lint:js",
    "lint:css": "stylelint './app/**/*.js'",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
    "lint:js": "npm run lint:eslint -- '**/*.js' '**/*.js.tmpl'",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prettify": "prettier --write"
  },
  "lint-staged": {
    "*.js": "npm run lint:eslint --"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "@material-ui/core": "^3.9.2",
    "@material-ui/styles": "^3.0.0-alpha.10",
    "@material-ui/system": "^3.0.0-alpha.2",
    "@sentry/browser": "^4.6.4",
    "assert": "^1.4.1",
    "autobind-decorator": "^2.3.1",
    "aws-sdk": "^2.348.0",
    "chalk": "^2.4.1",
    "classnames": "^2.2.6",
    "compression": "^1.7.3",
    "connected-react-router": "^5.0.0",
    "cross-env": "^5.2.0",
    "dedent": "^0.7.0",
    "dompurify": "^1.0.10",
    "elasticsearch-browser": "^15.2.0",
    "express": "^4.16.4",
    "fontfaceobserver": "^2.0.13",
    "highlight.js": "^9.13.1",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "intl": "^1.2.5",
    "invariant": "^2.2.4",
    "ip": "^1.1.5",
    "jss": "^10.0.0-alpha.13",
    "katex": "^0.10.0",
    "lodash": "^4.17.11",
    "minimist": "^1.2.0",
    "mixpanel-browser": "^2.23.0",
    "prop-types": "^15.6.2",
    "ramda": "^0.26.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-helmet": "^5.2.0",
    "react-images": "^0.5.19",
    "react-intl": "^2.7.2",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "react-stripe-checkout": "^2.6.3",
    "recompose": "^0.26.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-form": "^7.4.2",
    "redux-immutable": "^4.0.0",
    "redux-react-hook": "^3.1.0",
    "redux-saga": "^0.16.2",
    "remarkable": "^1.7.1",
    "reselect": "^4.0.0",
    "sanitize.css": "^4.1.0",
    "styled-components": "^3.4.10",
    "uuid": "^3.3.2",
    "vega-embed": "^3.22.1",
    "warning": "^4.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.4.3",
    "@babel/plugin-transform-react-constant-elements": "^7.2.0",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "acorn": "^6.0.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-react-intl": "^3.0.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.19",
    "bundlesize": "^0.17.1",
    "circular-dependency-plugin": "^5.0.2",
    "compare-versions": "^3.4.0",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "coveralls": "^3.0.2",
    "css-loader": "^0.28.11",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-redux-saga": "^0.9.0",
    "fetch-mock": "^6.5.2",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.3",
    "image-webpack-loader": "^4.4.0",
    "imports-loader": "^0.8.0",
    "jest-cli": "^23.6.0",
    "jest-localstorage-mock": "^2.3.0",
    "lint-staged": "^7.3.0",
    "ngrok": "^3.1.0",
    "node-plop": "^0.16.0",
    "null-loader": "^0.1.1",
    "plop": "^2.1.0",
    "prettier": "^1.14.3",
    "raf": "^3.4.1",
    "raw-loader": "^0.5.1",
    "react-app-polyfill": "^0.1.3",
    "react-test-renderer": "^16.6.0",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.2",
    "shelljs": "^0.8.2",
    "style-loader": "^0.23.1",
    "stylelint": "^9.6.0",
    "stylelint-config-recommended": "^2.1.0",
    "svg-url-loader": "^2.3.2",
    "terser-webpack-plugin": "^1.1.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-hot-middleware": "^2.24.3"
  }
}
